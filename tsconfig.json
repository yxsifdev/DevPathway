{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components": [
        "src/components/*"
      ],
      "@/helpers": [
        "src/helpers/*"
      ],
      "@/sections": [
        "src/sections/*"
      ],
      "@/utils": [
        "src/utils/*"
      ],
      "@/*": [
        "src/*"
      ]
    }
  }
}